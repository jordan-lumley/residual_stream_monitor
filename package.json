{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-win": "electron-builder --win",
    "dist-linux": "electron-builder",
    "build": "electron-packager . ResidualImport",
    "build-windows": "electron-packager --platform=win32 . ResidualImport"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "fast-csv": "^4.1.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "materialize-stepper": "^3.1.0",
    "moment": "^2.24.0",
    "objects-to-csv": "^1.3.6",
    "pg": "^8.0.0",
    "pg-promise": "^10.5.0",
    "pg-query-stream": "^3.0.4",
    "xlsx": "^0.15.6"
  },
  "build": {
    "appId": "ces.apps",
    "win": {
      "target": "msi"
    }
  }
}
